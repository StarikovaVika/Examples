{% extends "Base.twig" %}

{% block rows %}
	
	<div id="new_object_modal" class="modal fade"> 
		<div class="modal-dialog"> 
			<div class="modal-content"> 
				<div class="modal-header"> 
					<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button> 
					<h4 class="modal-title">Добавление нового оборудования</h4> 
				</div> 
				<div class="modal-body" id = "new_object_modal_body"> 
				<form id ="new_object_modal_form" enctype="multipart/form-data">
					<div class="form-group" id="object_name_fg">
						<label class="control-label">Название оборудования</label>
						<input type="text" class="form-control" id="object_name_input" required name="object_name" placeholder="Укажите название оборудования">
					</div>	
       
					<div class="form-group spec" id ="object_type_fg">
						<label class="control-label">Выберите тип оборудования</label>
						<select id ="object_type_select" required name="object_type" class="form-control"> 
							{% for type in types %}
								<option value="{{ type.pk_type }}">{{ type.type_name }}</option>
							{% endfor %} 
						</select> 
					</div>
					
					<div class="form-group" id="object_date_fg"> 
						<label class="control-label">Дата приобритения</label>
						<input type="date" class="form-control" id="object_date_input" required name="object_date" placeholder="Укажите дату приобретения">
					</div>						
							
					<div class="form-group" id="object_factory_number_fg">
						<label class="control-label">Заводской номер</label>
						<input type="number" class="form-control" id="object_factory_number_input" required name="object_factory_number" placeholder="Укажите заводской номер">
					</div>
					
					<div class="form-group" id="object_inventory_number_fg">
						<label class="control-label">Инвентарный номер</label>
						<input type="number" class="form-control" id="object_inventory_number_input" required name="object_inventory_number" placeholder="Укажите инвентарный номер">
					</div>
       					
					<div class="form-group" id="object_pasport_fg">
						<label class="control-label">Выберите файл фотографии паспорта оборудования</label>
						<input id ="object_pasport_input" required name="object_pasport" type="file" accept="image/*" placeholder="Выберите файл паспорта" >
					</div>	
					
				</form>
				</div> 
				
				<div class="modal-footer"> 
					<div class="btn-group" role="group" aria-label="...">
						<button type="button" class="btn btn-success" id = "new_object_add">Добавить</button> 
						<button type="button" class="btn btn-default" data-dismiss="modal">Закрыть</button> 
					</div>	
				</div> 
			</div> 
		</div> 
	</div>
				
