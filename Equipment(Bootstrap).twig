	<div class = "row">
		<div class="col-lg-4">		
			<div id="filter">
				<form id ="filter_param_form">
				<div id="filter_param">
					<h4>Фильтр</h4> 
					<div class="form-group"> 
						<label control-label>Наименование</label> 
						<input type="text" class="form-control" id="search_name" /> 
					</div> 
					
					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#toggleType" ><ins><strong>Тип оборудования</strong></ins></a> 					
						<div id="toggleType" class="collapse collapse-lvl1">				
							{% for t in types %} 
								<div class="checkbox"> 
									<label><input type="checkbox" value="{{t.pk_type}}">{{t.type_name}}</label> 
								</div> 
							{%endfor%} 
						</div> 
					</div> 
					
					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#toggleDate" ><ins><strong>Дата приобретения</strong></ins></a> 
						<div id="toggleDate" class="collapse collapse-lvl1"> 
							<div> 
								от<input type="Date" class="form-control" id = "begin"> 
								до<input type="Date" class="form-control" id = "end"> 
							</div> 
						</div> 
					</div> 
					
					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#toggleFactoryNumber" ><ins><strong>Заводской номер</strong></ins></a> 
						<div id="toggleFactoryNumber" class="collapse collapse-lvl1"> 
							<div> 
								<input type="text" class="form-control" id = "number"> 
							</div> 
						</div> 
					</div> 
					
					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#toggleInventoryNumber" ><ins><strong>Инвентарный номер</strong></ins></a> 
						<div id="toggleInventoryNumber" class="collapse collapse-lvl1"> 
							<div> 
								<input type="text" class="form-control" id = "number"> 
							</div> 
						</div> 
					</div> 
					
					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#toggleResponce" ><ins><strong>Эксплуатирующее лицо</strong></ins></a> 
						<div id="toggleResponce" class="collapse collapse-lvl1"> 
							{% for f in fios %} 
							<div class="checkbox"> 
								<label><input type="checkbox" value="{{f.pk_fio}}">{{f.surname}} {{f.fio_name}} {{f.patronymic}}</label> 
							</div> 
							{%endfor%} 
						</div> 
					</div> 

					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#togglePlace" ><ins><strong>Место расположения</strong></ins></a> 
						<div id="togglePlace" class="collapse collapse-lvl1"> 
							<div> 
								<a href="#" data-toggle="collapse" data-target="#toggleVsp" ><ins><strong>ВСП</strong></ins></a> 
								<div id="toggleVsp" class="collapse collapse-lvl2"> 
									<div> 
										{% for v in vsps %} 
										<div class="checkbox"> 
											<label><input type="checkbox" value="{{v.pk_vsp}}">{{v.name_vsp}}</label> 
										</div> 
										{%endfor%} 
									</div> 
								</div> 
							</div> 
							<div> 
								<a href="#" data-toggle="collapse" data-target="#toggleRoom" ><ins><strong>Кабинет</strong></ins></a> 
								<div id="toggleRoom" class="collapse collapse-lvl2"> 
									<div> 
										<label><input type="text" class="form-control" id = "number"></label> 
									</div> 
								</div> 
							</div> 
						</div> 
					</div> 
					<div class="form-group"> 
						<a href="#" data-toggle="collapse" data-target="#toggleStatus" ><ins><strong>Статус</strong></ins></a> 
						<div id="toggleStatus" class="collapse collapse-lvl1"> 
							{% for s in status %} 
							<div class="checkbox"> 
								<label><input type="checkbox" value="{{s.pk_status}}">{{s.name_status}}</label> 
							</div> 
							{%endfor%} 
						</div> 
					</div> 
					<hr> 				
				</div>
				</form>
				<div id="filter_print">
						<button type="button" id="search" class="btn btn-success"><span class="glyphicon glyphicon-search"></span> Найти</button> 
						<button type="button" id="clear_search" class="btn btn-success"><span class="glyphicon glyphicon-remove"></span> Сбросить фильтр</button> 
				</div>
			
			</div>
		
			<div id = "marker">
				<ul class="nav nav-pills nav-justified">
					<li id="nav_pills_1"><a href="#">1</a></li>
					<li id="nav_pills_2"><a href="#">2</a></li>
					<li id="nav_pills_3"><a href="#">3</a></li>
					<li id="nav_pills_4"><a href="#">4</a></li>
					<li id="nav_pills_5"><a href="#">5</a></li>
				</ul>		
			</div>	
			
			<div id="info">
				<div id="info_obj">				
				</div>				
			</div>
			
        </div>
		
		<div class="col-lg-8">
			<div id="group">
				<div class="col-lg-2">
					<fieldset id="foobar" data-val="1">						
						<label>Единичный выбор
							<input name="foobar" type="radio" value="one" checked  />
						</label>
						
						<label>Множественный выбор
							<input name="foobar" type="radio" value="lot" />
						</label>
					</fieldset>				 
				</div>
				
				<div class="col-lg-10">
					<div class="btn-group" role="group" aria-label="...">
						<button type="button" id="enter_btn" class="btn btn-success"> <span class="glyphicon glyphicon-plus"></span> Ввод</button>
						<button type="button" id="move_btn" class="btn btn-success" data-id="0"><span class="glyphicon glyphicon-road"></span> Перемещение</button>
						<button type="button" id="repair_btn" class="btn btn-success" data-id="0"><span class="glyphicon glyphicon-wrench"></span> Ремонт</button>
						<button type="button" id="write-off_btn" class="btn btn-success" data-id="0"><span class="glyphicon glyphicon-erase"></span> Списание</button>
						<button type="button" id="delete_btn" class="btn btn-success" data-id="0"><span class="glyphicon glyphicon-trash"></span> Утилизация</button>
						
					</div>
					
					<div class="col-lg-6" id ="lot_operation" hidden>	
						<label>Выберите данные для сортировки</label>
						<select id ="lot_operation_select" required name="lot_operation_select" class="form-control"> 
							<option value="1">Перемещение</option>
							<option value="2">Ремонт</option>
							<option value="3">Списание</option>
							<option value="4">Утилизация</option>
						</select> 
					</div>				
				</div>				
			</div>
		</div>
		
		<div class="col-lg-8">
			<div id="table">
				<div class="table-responsive">
					<table class="table table-hover table-bordered">
						<thead class ="table_head">
							<tr>
							  <th><span class="glyphicon glyphicon-th-list"></span></th>
							  <th>Наименование</th>
							  <th>Тип</th>
							  <th>Место</th>
							</tr>
						</thead>
						<tbody id="table_body">
							{% for object in objects %}
								<tr id="object_1" data-id="{{object.pk_object}}" name ="td_{{object.pk_object}}">
									<td> <input type="checkbox" name="group_chk_onc" class="hidden" data-id="{{object.pk_object}}" ></td>
									<td>{{object.name}}</td>
									<td>{{object.type_name}}</td>
									<td>ВСП: {{object.name_vsp}}, Кабинет: {{object.room}}</td>
								</tr>
							{% endfor %}
						</tbody>
					</table>
				</div>
			</div>
		</div> 
 		
	</div>		
</div>
{% endblock %}
